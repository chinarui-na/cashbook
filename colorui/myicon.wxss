@keyframes icon-spin {
    0% {
        -webkit-transform: rotate(0);
        transform: rotate(0);
    }

    100% {
        -webkit-transform: rotate(359deg);
        transform: rotate(359deg);
    }
}

.iconfont-spin {
    -webkit-animation: icon-spin 2s infinite linear;
    animation: icon-spin 2s infinite linear;
    display: inline-block;
}

.iconfont-pulse {
    -webkit-animation: icon-spin 1s infinite steps(8);
    animation: icon-spin 1s infinite steps(8);
    display: inline-block;
}

text[class*="icon-"],
view[class*="icon-"],
button[class*="icon-"] {
    font-family: "cuIcon" !important;
    font-size: inherit;
    font-style: normal;
}

@font-face {
  font-family: 'cuIcon';  /* project id 1219583 */
  src: url('//at.alicdn.com/t/font_1219583_mna68qrbpes.eot');
  src: url('//at.alicdn.com/t/font_1219583_mna68qrbpes.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1219583_mna68qrbpes.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1219583_mna68qrbpes.woff') format('woff'),
  url('//at.alicdn.com/t/font_1219583_mna68qrbpes.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1219583_mna68qrbpes.svg#iconfont') format('svg');
}

